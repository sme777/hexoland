                Studio
                <% end %>
            </li>
            <li class="nav-item">
                <%= link_to docs_path, class: (tab == "Docs" ? "nav-link active" : "nav-link text-white")  do %>
                <svg class="bi pe-none me-2" width="16" height="16">
                    <use xlink:href="#docs" /></svg>
                Docs
                <% end %>
            </li>
            
        </ul>
        <hr>
        <div class="dropdown">
            <% if !!@user %>
                <a href="#" class="d-flex align-items-center text-white text-decoration-none dropdown-toggle"
                data-bs-toggle="dropdown" aria-expanded="false">
                <img src="https://github.com/sme777.png" alt="" width="32" height="32" class="rounded-circle me-2">
                <strong>eecsgod</strong>
                </a>
                <ul class="dropdown-menu dropdown-menu-dark text-small shadow">
                <li><a class="dropdown-item" href="#">New project...</a></li>
                <li><a class="dropdown-item" href="#">Settings</a></li>
                <li><a class="dropdown-item" href="#">Profile</a></li>
                <li>
                    <hr class="dropdown-divider">
                </li>
                <li><a class="dropdown-item" href="#">Sign out</a></li>
                </ul>
            <% else %>
                <h3>Sign In</h3>
                <div class="thirdparty-login-container row py-2">

                    <div class="x-login-container col-md-6">
                        <%= form_with url: sign_in_with_x_path, method: "post" do |f| %>
                            <%= f.button class: "btn btn-lg btn-outline-primary bg-primary-subtle w-100", type: "submit" do %>
                                <svg xmlns="http://www.w3.org/2000/svg" class="bi pe-none me-2" width="24" height="24" viewBox="0 0 512 512">
                                <path d="M389.2 48h70.6L305.6 224.2 487 464H345L233.7 318.6 106.5 464H35.8L200.7 275.5 26.8 48H172.4L272.9 180.9 389.2 48zM364.4 421.8h39.1L151.1 88h-42L364.4 421.8z"/>
